<div class="profil">
    <div class="row">
        <div class="col"></div>
        <div class="div col col-md-6">

           <h1>Bienvenue sur le profile Manager</h1>
            <%= form_with url: "/profil/search", method: :post do |form| %>
                <%= form.label :pseudo, "Pseudo Minecraft:" %>
                <%= form.text_field :pseudo %>
                <%= form.submit "Search" %>
            <% end %>
        </div>
        <div class="col"></div>
    </div>
</div>